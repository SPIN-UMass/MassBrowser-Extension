{
	"name": "__MSG_extensionName__",
	"version": "0.1.0",
	"manifest_version": 2,
	"author": "Massbrowser-Group",
	"description": "__MSG_extensionDescription__",
	"homepage_url": "http://massbowser.cs.umass.edu",
	"default_locale": "en",
	"icons": {
		"16": "icons/mb-16.png",
		"24": "icons/mb-24.png",
		"48": "icons/mb-48.png",
		"96": "icons/mb-96.png",
		"128": "icons/mb-128.png"
	},
	"permissions": [
		"<all_urls>",
		"activeTab",
		"tabs",
		"proxy",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"unlimitedStorage",
		"notifications"
	],
	"browser_action": {
		"browser_style": true,
		"default_title": "__MSG_extensionName__",
		"default_popup": "ui/popup.html",
		"default_icon": {
			"16": "icons/mb-16.png",
			"24": "icons/mb-24.png",
			"48": "icons/mb-48.png",
			"96": "icons/mb-96.png",
			"128": "icons/mb-128.png"
		}
	},
	"background": {
		"scripts": [
			"ui/js/pako.min.js",
			"core.js"
		]
	},
	"options_ui": {
		"page": "ui/settings.html",
		"open_in_tab": true,
		"browser_style": false
	},
	"applications": {
		"gecko": {
			"id": "milad@cs.umass.edu",
			"strict_min_version": "60.0"
		}
	}
}